[{
    "groups": {
        "remove": [{"section":"infotext"}],
        "move": [{"section": "sectionDetails", "before":  "gradeSummary" }]
    },
    "sections": {
        "finalCourseDetailGrid": {
            "remove": [ {"field":"courseReferenceNumberx"}, {"field":"termx"}, {"field":"subjectx"}, {"field":"courseTitlex"} ],
            "move": [
                {"field": "courseTitlex", "before": "courseReferenceNumberx"},
                {"field": "courseReferenceNumberx", "before": "termx"},
                {"field": "termx", "before": "subjectx"},
                {"field": "subjectx", "before": "gradedStatusx"}
            ],
            "replace": [{"field":"adjustedServiceDate",
                "html": "<div class=\"col-md-12 col-lg-12 col-sm-12 col-xs-12 no-padding\" data-xe-field=\"adjustedServiceDate\"><p><strong>Adj. Service Date:</strong> {{sectionData.employeeSummary.adjustedServiceDate | myDateFormat : 'moment.default.date.format'}}</p>\n</div>"}
            ]
        },
        "courseDetails": {
            "move": [ {"field":"-eligible-", "after":"enrolled"}],
            "remove": [ {"field":"courseTitle"}],
            "replace": [{"field": "courseReferenceNumber","label": "Course ref..."},
                        {"field": "eligible","label": {"key":"courseDetails.eligible.label"}}]
        },
        "midtermCourseDetailGrid": {
            "remove": [ {"field":"bannerId"} ]
        },
        "numbersTest": {
            "remove": [{"field":"numberInput"},
                        {"field":"inputNumericNormal"} ]
        },
        "finalGradeCourseRoster": {
            "replace": [ {"field":"searchTextBox", "placeholder": {"key":"finalGradeCourseRoster.searchTextBox.placeholder"},
                          "title": {"key":"finalGradeCourseRoster.searchTextBox.title"}}]
        }
    }
}]
